; transforms
(clear)
(transform:move 150 150)
(fill 
  (circle 0 0 150) "#ffb545")
(transform:move 300 0)
(transform:rotate 
  (rad 90))
(fill 
  (circle 0 0 150) "white")
(transform:move 300 0)
(transform:rotate 
  (rad 90))
(fill 
  (circle 0 0 150) "#72dec2")
(transform:move 300 0)
(fill 
  (circle 0 0 150) "#555")
(transform:reset)